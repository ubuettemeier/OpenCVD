
.PHONY: clean

all:
	$(MAKE)  -C basic/
	$(MAKE)  -C edge/
	$(MAKE)  -C filtering/
	$(MAKE)  -C findcontours/
	$(MAKE)  -C imread/
	$(MAKE)  -C short_sample/
	$(MAKE)  -C demhist/
	$(MAKE)  -C HoughCircles/
	$(MAKE)  -C HoughLines/
clean:
	$(MAKE)  -C basic/  clean
	$(MAKE)  -C edge/  clean
	$(MAKE)  -C filtering/  clean
	$(MAKE)  -C findcontours/  clean
	$(MAKE)  -C imread/  clean
	$(MAKE)  -C short_sample/  clean
	$(MAKE)  -C demhist/  clean
	$(MAKE)  -C HoughCircles/ clean
	$(MAKE)  -C HoughLines/ clean

